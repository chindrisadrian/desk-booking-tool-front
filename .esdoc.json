{
    "source": "./src",
    "destination": "./docs",
    "plugins": [
      {
        "name": "esdoc-standard-plugin"
      },
      {
        "name": "esdoc-coverage-plugin",
        "option": {
          "enable": true,
          "kind": [
            "class",
            "method",
            "member",
            "get",
            "set",
            "constructor",
            "function",
            "variable"
          ]
        }
      },
      {
        "name": "esdoc-ecmascript-proposal-plugin",
        "option": { "all": true }
      },
      {
        "name": "esdoc-flow-plugin"
      },
      { "name": "esdoc-jsx-plugin", "option": { "enable": true } }
    ]
  }